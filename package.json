{
  "name": "playwright-ai-starter",
  "version": "1.0.0",
  "scripts": {
  "test:quick": "npx playwright test --project=chromium",
  "test": "playwright test",
  "test:chrome": "playwright test --project=chromium",
  "test:ff": "playwright test --project=firefox",
  "test:safari": "playwright test --project=webkit",

  "test:bing": "SEARCH_ENGINE=bing npx playwright test --project=chromium --headed -- tests/search-engine.spec.ts",
  "test:ddg": "SEARCH_ENGINE=duckduckgo npx playwright test --project=chromium --headed -- tests/search-engine.spec.ts",
  "test:google": "SEARCH_ENGINE=google npx playwright test --project=chromium --headed -- tests/search-engine.spec.ts",

  "test:headed": "playwright test --project=chromium --headed",
  "debug": "PWDEBUG=1 playwright test --project=chromium",

  "report": "playwright show-report",
  "trace": "playwright show-trace test-results/**/*.zip",

  
  "test:chromium": "playwright test --project=chromium",
  "test:ui": "playwright test --ui",
  "update-snapshots": "playwright test --update-snapshots"
},

  "devDependencies": {
    "@playwright/test": "^1.56.0",
    "@types/node": "^24.7.0",
    "dotenv": "^17.2.3",
    "playwright": "^1.56.0",
    "tsx": "^4.20.6"
  }
}
